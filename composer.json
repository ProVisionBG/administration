{
  "version": "0.3.1",
  "name": "provision/administration",
  "description": "ProVision Administration - laravel package",
  "keywords": [
    "administration",
    "modular",
    "laravel"
  ],
  "license": "Apache License 2.0",
  "authors": [
    {
      "name": "Venelin Iliev",
      "homepage": "http://veneliniliev.com"
    }
  ],
  "support": {
    "issues": "https://github.com/ProVisionBG/administration/issues",
    "source": "https://github.com/ProVisionBG/administration"
  },
  "homepage": "http://provision.bg",
  "type": "library",
  "require": {
    "php": "^5.6.4 || ^7.0",
    "laravel/framework": "5.5.*",
    "provision/minifier": "dev-master",
    "provision/media-manager": "v5.5.x-dev",
    "provision/meta-tags": "dev-master",
    "provision/visual-builder": "dev-master",
    "provision/translation": "dev-master",
    "mcamara/laravel-localization": "^1.3",
    "zizaco/entrust": "5.2.x-dev",
    "caffeinated/modules": "^4.2",
    "lavary/laravel-menu": "^1.7",
    "cviebrock/eloquent-sluggable": "^4.3",
    "provision/laravel-meta-tags": "^3.0",
    "provision/breadcrumbs": "dev-master",
    "yajra/laravel-datatables-oracle": "^7.6",
    "yajra/laravel-datatables-html": "^1.0",
    "edvinaskrucas/notification": "5.*",
    "laravel/socialite": "^2.0",
    "intervention/image": "^2.3",
    "kalnoy/nestedset": "^4.1",
    "davejamesmiller/laravel-breadcrumbs": "^3.0",
    "barryvdh/laravel-translation-manager": "^0.4.1",
    "venturecraft/revisionable": "1.*",
    "laravelcollective/html": "^5.3",
    "kris/laravel-form-builder": "^1.7",
    "dimsav/laravel-translatable": "8.*",
    "rutorika/sortable": "^4.1",
    "barryvdh/laravel-debugbar": "^3.1",
    "arcanedev/log-viewer": "^4.4",
    "caouecs/laravel-lang": "~3.0",
    "artesaos/seotools": "^0.10.0"
  },
  "require-dev": {
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "ProVision\\Administration\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "ProVision\\Administration\\": "app/",
      "ProVision\\Administration\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ProVision\\Administration\\Providers\\AdministrationServiceProvider"
      ],
      "aliases": {
        "Administration": "ProVision\\Administration\\Facades\\Administration",
        "AdministrationMenu": "ProVision\\Administration\\Facades\\AdministrationMenu",
        "Settings": "ProVision\\Administration\\Facades\\Settings",
        "Dashboard": "ProVision\\Administration\\Dashboard"
      }
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan vendor:publish --force --provider=\"ProVision\\Administration\\Providers\\AdministrationServiceProvider\"",
      "php artisan admin:migrate"
    ],
    "post-update-cmd": [
      "php artisan vendor:publish --tag=views --tag=public --tag=lang --tag=migrations --force --provider=\"ProVision\\Administration\\Providers\\AdministrationServiceProvider\"",
      "php artisan admin:migrate"
    ],
    "test": "phpunit"
  }
}
