{
  "version": "0.0.1",
  "name": "provision/administration",
  "description": "ProVision Administration - laravel package",
  "keywords": [
    "administration",
    "modular",
    "laravel"
  ],
  "authors": [
    {
      "name": "Venelin Iliev",
      "email": "venelin@provision.bg",
      "homepage": "http://veneliniliev.com"
    }
  ],
  "support": {
    "issues": "https://github.com/ProVisionBG/administration/issues",
    "source": "https://github.com/ProVisionBG/administration"
  },
  "homepage": "http://provision.bg",
  "type": "library",
  "require": {
    "php": "^5.5 || ^7.0",
    "laravel/framework": "5.2.*",
    "mcamara/laravel-localization": "^1.1",
    "zizaco/entrust": "5.2.x-dev",
    "caffeinated/modules": "^3.1",
    "lavary/laravel-menu": "^1.6",
    "yajra/laravel-datatables-oracle": "^6.10",
    "cviebrock/eloquent-sluggable": "^4.0.2",
    "torann/laravel-meta-tags": "^3.0",
    "edvinaskrucas/notification": "5.*",
    "laravel/socialite": "^2.0",
    "intervention/image": "^2.3",
    "kalnoy/nestedset": "^4.1",
    "davejamesmiller/laravel-breadcrumbs": "^3.0",
    "barryvdh/laravel-translation-manager": "^0.2.2",
    "venturecraft/revisionable": "1.*",
    "laravelcollective/html": "~5.0",
    "kris/laravel-form-builder": "^1.7",

    "fxp/composer-asset-plugin": "~1.1",
    "bower-asset/bootstrap": "^3.3.6",
    "bower-asset/AdminLTE": "^2.3.3",
    "bower-asset/font-awesome": "^4.6.3",
    "bower-asset/Ionicons": "^2.0.1",
    "bower-asset/iCheck": "^1.0.2",
    "bower-asset/html5shiv": "^3.7.3",
    "bower-asset/respond": "^1.4.2",
    "bower-asset/datatables": "^1.10.12",
    "bower-asset/jquery-ui": "^1.11.4",
    "bower-asset/dropzone": "^4.3.0",
    "bower-asset/jqueryui-timepicker-addon": "^1.6.3",
    "bower-asset/jquery-slimscroll": "^1.3.8",
    "bower-asset/fastclick": "^1.0.6",
    "bower-asset/ckeditor": "^4.5.9",
    "bower-asset/datatables-responsive": "^2.1.0",
    "bower-asset/datatables-rowreorder": "^1.1.2",
    "bower-asset/jquery-validation": "^1.15.0"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "ProVision\\Administration\\": "app/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan vendor:publish --tag=public --force"
    ],
    "post-update-cmd": [
      "php artisan vendor:publish --tag=public --force"
    ]
  },
  "extra": {
    "asset-installer-paths": {
      "bower-asset-library": "public/vendor/provision/administration/bower_components/"
    }
  }
}
