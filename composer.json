{
  "version": "0.0.1",
  "name": "provision/administration",
  "description": "ProVision Administration - laravel package",
  "keywords": [
    "administration",
    "modular",
    "laravel"
  ],
  "authors": [
    {
      "name": "Venelin Iliev",
      "email": "venelin@provision.bg",
      "homepage": "http://veneliniliev.com"
    }
  ],
  "support": {
    "issues": "https://github.com/ProVisionBG/administration/issues",
    "source": "https://github.com/ProVisionBG/administration"
  },
  "homepage": "http://provision.bg",
  "type": "library",
  "minimum-stability": "stable",
  "require": {
    "php": "^5.5 || ^7.0",
    "laravel/framework": "5.2.*",
    "mcamara/laravel-localization": "^1.1",
    "zizaco/entrust": "5.2.x-dev",
    "caffeinated/modules": "^3.1",
    "lavary/laravel-menu": "^1.6",
    "yajra/laravel-datatables-oracle": "^6.10",
    "cviebrock/eloquent-sluggable": "^4.0.2",
    "torann/laravel-meta-tags": "^3.0",
    "edvinaskrucas/notification": "5.*",
    "laravel/socialite": "^2.0",
    "intervention/image": "^2.3",
    "kalnoy/nestedset": "^4.1",
    "davejamesmiller/laravel-breadcrumbs": "^3.0",
    "barryvdh/laravel-translation-manager": "^0.2.2",
    "venturecraft/revisionable": "1.*",
    "laravelcollective/html": "~5.0",
    "kris/laravel-form-builder": "^1.7",
    "eloquent/composer-npm-bridge": "dev-develop",
    "php-kit/composer-bower-plugin": "^1.0"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "ProVision\\Administration\\": "app/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan vendor:publish --tag=public --force"
    ],
    "post-update-cmd": [
      "php artisan vendor:publish --tag=public --force"
    ]
  },
  "extra": {
    "bower": {
      "require": {
        "AdminLTE": "admin-lte#^2.3.3",
        "bootstrap": "^3.3.6",
        "font-awesome": "^4.6.3",
        "Ionicons": "ionicons#^2.0.1",
        "iCheck": "^1.0.2",
        "html5shiv": "^3.7.3",
        "respond": "^1.4.2",
        "datatables": "^1.10.12",
        "jquery-ui": "^1.11.4",
        "dropzone": "^4.3.0",
        "jqueryui-timepicker-addon": "^1.6.3",
        "jquery-slimscroll": "slimScroll#^1.3.8",
        "fastclick": "^1.0.6",
        "ckeditor": "^4.5.9",
        "datatables-responsive": "^2.1.0",
        "datatables-rowreorder": "^1.1.2",
        "jquery-validation": "^1.15.0"
      },
      "directory": "public/vendor/provision/administration/bower_components",
      "scripts": {
      }
    }
  }
}
